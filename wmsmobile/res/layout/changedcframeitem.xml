<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="#FFFFFF"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/v5_0_1_friends_item_bg"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/changedcframeitem_unique_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="8dip"
            android:layout_marginTop="0dip"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize" />

        <TextView
            android:id="@+id/changedcframeitem_item_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="8dip"
            android:layout_marginTop="0dip"
            android:layout_toRightOf="@+id/changedcframeitem_unique_id"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize" />

        <TextView
            android:id="@+id/changedcframeitem_subinv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="8dip"
            android:layout_marginTop="0dip"
            android:layout_toRightOf="@+id/changedcframeitem_item_name"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize" />

        <TextView
            android:id="@+id/changedcframeitem_onhandqty"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="8dip"
            android:layout_marginTop="0dip"
            android:layout_toRightOf="@+id/changedcframeitem_subinv"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize" />

        <TextView
            android:id="@+id/changedcframeitem_simulatedqty"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="8dip"
            android:layout_marginTop="0dip"
            android:layout_toRightOf="@+id/changedcframeitem_onhandqty"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize" />

        <TextView
            android:id="@+id/changedcframeitem_datecode"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="8dip"
            android:layout_marginTop="0dip"
            android:layout_toRightOf="@+id/changedcframeitem_simulatedqty"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize" />

        <TextView
            android:id="@+id/changedcframeitem_frame"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="8dip"
            android:layout_marginTop="0dip"
            android:layout_toRightOf="@+id/changedcframeitem_datecode"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize" />
    </RelativeLayout>

</LinearLayout>