<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="#FFFFFF"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/v5_0_1_friends_item_bg"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/wipturnin_query_unique_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="@dimen/colmargin"
            android:layout_marginTop="0dip"
            android:paddingBottom="0dip"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize"
            />

        <TextView
            android:id="@+id/wipturnin_query_invoice_no"
            android:layout_toRightOf="@+id/wipturnin_query_unique_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="@dimen/colmargin"
            android:layout_marginTop="0dip"
            android:paddingBottom="0dip"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize"
            />

        <TextView
            android:id="@+id/wipturnin_query_wo_no"
            android:layout_toRightOf="@+id/wipturnin_query_invoice_no"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="@dimen/colmargin"
            android:layout_marginTop="0dip"
            android:paddingBottom="0dip"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize"
            />

        <TextView
            android:id="@+id/wipturnin_query_region_name"
            android:layout_toRightOf="@+id/wipturnin_query_wo_no"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="@dimen/colmargin"
            android:layout_marginTop="0dip"
            android:paddingBottom="0dip"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize"
            />

        <TextView
            android:id="@+id/wipturnin_query_part_no"
            android:layout_toRightOf="@+id/wipturnin_query_region_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="@dimen/colmargin"
            android:layout_marginTop="0dip"
            android:paddingBottom="0dip"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize"
            />

        <TextView
            android:id="@+id/wipturnin_query_required_qty"
            android:layout_toRightOf="@+id/wipturnin_query_part_no"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="@dimen/colmargin"
            android:layout_marginTop="0dip"
            android:paddingBottom="0dip"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize"
             />

        <TextView
            android:id="@+id/wipturnin_query_balance_qty"
            android:layout_toRightOf="@+id/wipturnin_query_required_qty"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="@dimen/colmargin"
            android:layout_marginTop="0dip"
            android:paddingBottom="0dip"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize"
             />


        <TextView
            android:id="@+id/wipturnin_query_subinventory_name"
            android:layout_toRightOf="@+id/wipturnin_query_balance_qty"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="@dimen/colmargin"
            android:layout_marginTop="0dip"
            android:paddingBottom="0dip"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize"
             />

        <TextView
            android:id="@+id/wipturnin_query_locator_name"
            android:layout_toRightOf="@+id/wipturnin_query_subinventory_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="@dimen/colmargin"
            android:layout_marginTop="0dip"
            android:paddingBottom="0dip"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize"
             />

        <TextView
            android:id="@+id/wipturnin_query_version"
            android:layout_toRightOf="@+id/wipturnin_query_locator_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="@dimen/colmargin"
            android:layout_marginTop="0dip"
            android:paddingBottom="0dip"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize"
             />

            <TextView
            android:id="@+id/wipturnin_query_frame_name"
            android:layout_toRightOf="@+id/wipturnin_query_version"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="@dimen/colmargin"
            android:layout_marginTop="0dip"
            android:paddingBottom="0dip"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize"
             />
     
    </RelativeLayout>

</LinearLayout>