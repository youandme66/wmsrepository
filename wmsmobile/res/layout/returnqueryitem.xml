<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="#FFFFFF"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/v5_0_1_friends_item_bg"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/returnquery_org"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="@dimen/colmargin"
            android:layout_marginTop="0dip"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize" />

        <TextView
            android:id="@+id/returnquery_invoice_no"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="@dimen/colmargin"
            android:layout_marginTop="0dip"
            android:layout_toRightOf="@+id/returnquery_org"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize" />

        <TextView
            android:id="@+id/returnquery_pn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="@dimen/colmargin"
            android:layout_marginTop="0dip"
            android:layout_toRightOf="@+id/returnquery_invoice_no"
            android:paddingBottom="0dip"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize" />

        <TextView
            android:id="@+id/returnquery_require"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="@dimen/colmargin"
            android:layout_marginTop="0dip"
            android:layout_toRightOf="@+id/returnquery_pn"
            android:paddingBottom="0dip"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize" />

        <TextView
            android:id="@+id/returnquery_balance"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="@dimen/colmargin"
            android:layout_marginTop="0dip"
            android:layout_toRightOf="@+id/returnquery_require"
            android:paddingBottom="0dip"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize" />

        <TextView
            android:id="@+id/returnquery_sub"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="@dimen/colmargin"
            android:layout_marginTop="0dip"
            android:layout_toRightOf="@+id/returnquery_balance"
            android:paddingBottom="0dip"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize" />

        <TextView
            android:id="@+id/returnquery_loc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="@dimen/colmargin"
            android:layout_marginTop="0dip"
            android:layout_toRightOf="@+id/returnquery_sub"
            android:paddingBottom="0dip"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize" />

        <TextView
            android:id="@+id/returnquery_transaction_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="@dimen/colmargin"
            android:layout_marginTop="0dip"
            android:layout_toRightOf="@+id/returnquery_loc"
            android:paddingBottom="0dip"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize" />

        <TextView
            android:id="@+id/returnquery_dept_code"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="@dimen/colmargin"
            android:layout_marginTop="0dip"
            android:layout_toRightOf="@+id/returnquery_transaction_type"
            android:paddingBottom="0dip"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize"
            android:visibility="visible" />

        <TextView
            android:id="@+id/returnquery_wo_no"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="@dimen/colmargin"
            android:layout_marginTop="0dip"
            android:paddingBottom="0dip"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/returnquery_wo_key"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="@dimen/colmargin"
            android:layout_marginTop="0dip"
            android:layout_toRightOf="@+id/returnquery_wo_no"
            android:paddingBottom="0dip"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/returnquery_route"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="@dimen/colmargin"
            android:layout_marginTop="0dip"
            android:layout_toRightOf="@+id/returnquery_wo_key"
            android:paddingBottom="0dip"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/returnquery_reason_code"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="@dimen/colmargin"
            android:layout_marginTop="0dip"
            android:layout_toRightOf="@+id/returnquery_route"
            android:paddingBottom="0dip"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/returnquery_item_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="@dimen/colmargin"
            android:layout_marginTop="0dip"
            android:layout_toRightOf="@+id/returnquery_reason_code"
            android:paddingBottom="0dip"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/returnquery_pu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="@dimen/colmargin"
            android:layout_marginTop="0dip"
            android:layout_toRightOf="@+id/returnquery_item_id"
            android:paddingBottom="0dip"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/returnquery_reason_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="@dimen/colmargin"
            android:layout_marginTop="0dip"
            android:layout_toRightOf="@+id/returnquery_pu"
            android:paddingBottom="0dip"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/returnquery_remark"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="@dimen/colmargin"
            android:layout_marginTop="0dip"
            android:layout_toRightOf="@+id/returnquery_dept_code"
            android:paddingBottom="0dip"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/returnquery_batch_line_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="@dimen/colmargin"
            android:layout_marginTop="0dip"
            android:layout_toRightOf="@+id/returnquery_remark"
            android:paddingBottom="0dip"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/returnquery_return_line_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="@dimen/colmargin"
            android:layout_marginTop="0dip"
            android:layout_toRightOf="@+id/returnquery_transaction_type"
            android:paddingBottom="0dip"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/returnquery_suffocate_code"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="false"
            android:layout_marginBottom="0dip"
            android:layout_marginLeft="@dimen/colmargin"
            android:layout_marginTop="0dip"
            android:layout_toRightOf="@+id/returnquery_return_line_id"
            android:paddingBottom="0dip"
            android:textColor="@color/textview_color"
            android:textSize="@dimen/textview_textsize"
            android:visibility="invisible" />
    </RelativeLayout>

</LinearLayout>